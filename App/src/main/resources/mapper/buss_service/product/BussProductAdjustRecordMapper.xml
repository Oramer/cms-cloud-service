        <result property="no14" column="no14"/>
        <result property="no15" column="no15"/>
    </resultMap>

    <sql id="selectBussProductAdjustRecordVo">
        SELECT rd.pk,
        (SELECT product_name FROM buss_product_info where pk = rd.product_pk) as product_pk,
        rd.adjust_price,
        (SELECT nick_name FROM sys_user where user_id = rd.target_pk) as  target_pk,
        rd.reversion, rd.create_by, rd.create_time,
        rd.update_by, rd.update_time, rd.no1, rd.no2, rd.no3, rd.no4, rd.no5,
        rd.no6, rd.no7, rd.no8, rd.no9, rd.no10, rd.no11, rd.no12, rd.no13, rd.no14, rd.no15
        FROM buss_product_adjust_record AS rd
        LEFT JOIN sys_user u ON u.user_id  =  rd.create_by
        LEFT JOIN sys_dept d ON u.dept_id  =  d.dept_id

    </sql>

    <select id="selectBussProductAdjustRecordList"
            parameterType="com.cms.buss_service.product.domain.BussProductAdjustRecord"
            resultMap="BussProductAdjustRecordResult">
        <include refid="selectBussProductAdjustRecordVo"/>
        <where>
            1 = 1
            <if test="productPk != null  and productPk != ''">AND rd.product_pk = #{productPk}</if>
            <if test="adjustPrice != null ">AND rd.adjust_price = #{adjustPrice}</if>
            <if test="targetPk != null  and targetPk != ''">AND rd.target_pk = #{targetPk}</if>
            <if test="reversion != null  and reversion != ''">AND rd.reversion = #{reversion}</if>
            <if test="no1 != null  and no1 != ''">AND rd.no1 = #{no1}</if>
            <if test="no2 != null  and no2 != ''">AND rd.no2 = #{no2}</if>
            <if test="no3 != null  and no3 != ''">AND rd.no3 = #{no3}</if>
            <if test="no4 != null  and no4 != ''">AND rd.no4 = #{no4}</if>
            <if test="no5 != null  and no5 != ''">AND rd.no5 = #{no5}</if>
            <if test="no6 != null  and no6 != ''">AND rd.no6 = #{no6}</if>
            <if test="no7 != null  and no7 != ''">AND rd.no7 = #{no7}</if>
            <if test="no8 != null  and no8 != ''">AND rd.no8 = #{no8}</if>
            <if test="no9 != null  and no9 != ''">AND rd.no9 = #{no9}</if>
            <if test="no10 != null  and no10 != ''">AND rd.no10 = #{no10}</if>
            <if test="no11 != null  and no11 != ''">AND rd.no11 = #{no11}</if>
            <if test="no12 != null  and no12 != ''">AND rd.no12 = #{no12}</if>
            <if test="no13 != null  and no13 != ''">AND rd.no13 = #{no13}</if>
            <if test="no14 != null  and no14 != ''">AND rd.no14 = #{no14}</if>
            <if test="no15 != null  and no15 != ''">AND rd.no15 = #{no15}</if>
        </where>
        ${dataScope}
        ORDER BY rd.create_time DESC
    </select>

    <select id="selectBussProductAdjustRecordById" parameterType="Long" resultMap="BussProductAdjustRecordResult">
        SELECT * FROM buss_product_adjust_record AS rd
        WHERE rd.pk = #{pk}
    </select>

    <insert id="insertBussProductAdjustRecord"
